import numpy as np
import pandas as pd

df=pd.read_csv('C:/Users/joadp/Desktop/Twilight of the Data Gods/csv_files/train.csv')
ds=df.values

H=[]
D=[]
M=[]

for i in range(len(ds[:,2])):
    h=float(ds[:,2][i][11:13])
    d=float(ds[:,2][i][8:10])
    m=float(ds[:,2][i][5:7])
    H.append(h)
    D.append(d)
    M.append(m)
H=np.array(H)
D=np.array(D)
M=np.array(M)

train=np.zeros((20216100, 6))
train[:,0]=ds[:,0]
train[:,1]=ds[:,1]
train[:,2]=M
train[:,3]=D
train[:,4]=H
train[:,5]=ds[:,3]
